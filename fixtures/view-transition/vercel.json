{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "outputDirectory": "build/static",
  "buildCommand": "",
  "env": {
    "BABEL_DISABLE_CACHE": "1",
    "DISABLE_ESLINT_PLUGIN": "true"
  },
  "functions": {
    "api/index.js": {
      "includeFiles": "src/**/*"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ]
}
